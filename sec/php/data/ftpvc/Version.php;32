<?php
/**
 * Version
 */
class Version {
  //
  const MAJOR = "4.0";  
  const PROD_DEPLOY = "05l";  
  const TEST_DEPLOY = "3"; 
  //
  public static function getLabel() {
    return Version::MAJOR . "." . Version::PROD_DEPLOY;
  }
  public static function getUrlSuffix() {
    return Version::MAJOR . "p" . Version::PROD_DEPLOY . "t" . Version::TEST_DEPLOY;
  }
}
