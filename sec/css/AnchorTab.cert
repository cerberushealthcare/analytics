DIV.at-shadow {
  border:none;
  background-color:#6ABC76;
  border:1px solid white;
  visibility:hidden;
  position:absolute;
  top:0;
  left:0;
  z-index:500;
}
A.at-anchor {
  background-color:#C7E5CB;
  color:black;
  text-decoration:none;
  white-space:nowrap;
  border-top:3px solid #C7E5CB;
  border-bottom:3px solid #C7E5CB;
  border-left:5px solid #C7E5CB;
  border-right:5px solid #C7E5CB;
  visibility:hidden;
  position:absolute;
  top:0;
  left:0;
  z-index:502;
  cursor:default;
}
DIV.at-panel {
  padding:5px;
  border:4px solid #C7E5CB;
  background-color:#ECF6ED;
  text-align:left;
  visibility:hidden;
  position:absolute;
  top:0;
  left:0;
  z-index:501;
}
DIV.at-panel A.cmd {
  border:1px solid #B1A89B;
}
DIV.at-panel A.cmd:hover {
  border-color:#F42941;
}
DIV.at-panel DIV.pop-cmd {
  margin-top:5px;
}
DIV.at-panel LABEL.lcheck-on {
  background-color:yellow;
}