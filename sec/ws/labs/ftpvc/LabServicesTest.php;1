<?php
set_include_path('../../');
require_once 'php/data/rec/sql/dao/Logger.php';
/*
 * Lab Web Services
 * @author Warren Hornsby
 */
ini_set("soap.wsdl_cache_enabled", "0"); // disabling WSDL cache
$server = new SoapServer('LabServicesTest.wsdl');
$server->addFunction('postMessage');
$server->handle();
function postMessage($msg) {
  Logger::debug_r($msg, 'LabServicesTest::postMessage');
  $login = false;
  if (isset($msg->credentials)) {
    if ($msg->credentials->id = 'wpml' && $msg->credentials->password == 'zhtest')
      $login = true;
  }
  if ($login == false) {
    $result = new stdClass();
    $result->status = 'FAIL';
    $result->errorMsg = 'Invalid credentials';
    return $result;
  }
  $result = new stdClass();
  $result->status = 'OK';
  return $result;
}

