class SXCM_TS extends TS {
  public /*cs_SetOperator*/ $_operator;
  //
  static function fromMed($med) {
    $freqInHours = $med->getFreqInHours();
    $dateTo = $med->expires;
    // TODO: how to get dateFrom, and how to get dateTo from NewCrop?
    return self::asSubstanceAdmin($freqInHours, null, $dateTo);
  }
  static function asSubstanceAdmin($freqInHours, $dateFrom = null, $dateTo = null) {
    $e1 = PIVL_TS::asSubstanceAdminFreq($freqInHours);
    $e1->setXsiType();
    if ($dateFrom == null && $dateTo == null) {
      return $e1;
    } else {
      $e1->_operator = 'A';
      $e2 = IVL_TS::asLowHigh($dateFrom, $dateTo);
      $e2->setXsiType();
      return array($e2, $e1);
    }
  }
}
class IVL_TS extends SXCM_TS {
  //
  public /*IVXB_TS*/ $low; //REQ 
  public /*PQ*/ $width;
  public /*IVXB_TS*/ $high; 
  public /*TS*/ $center; //REQ
  //
  static function asLowHigh($dateFrom, $dateTo) {
    $e = new static();
    $e->low = IVXB_TS::fromDate($dateFrom);
    $e->high= IVXB_TS::fromDate($dateTo);
    return $e;
  }
}
