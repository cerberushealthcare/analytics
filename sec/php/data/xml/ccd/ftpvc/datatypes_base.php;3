class ANY extends XmlRec {
  public $_xsi_type;
  public /*cs_NullFlavor*/ $_nullFlavor;
  //
  public function setXsiType($class = null) {
    if ($class == null) { 
      $a = explode('_', get_class($this));
      $class = $a[0];
    }
    $this->_xsi_type = $class;
  }
  //
  static function asNull($flavor) {
    $e = new static();
    $e->_nullFlavor = $flavor;
    return $e;  
  }
}
