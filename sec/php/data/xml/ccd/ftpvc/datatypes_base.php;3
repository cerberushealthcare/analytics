class TS extends QTY {
  public /*ts*/ $_value;
  //
  static function fromDate($date = null) {
    $e = new static();
    if ($date && strlen($date) <= 8) {
      $e->_value = $date;
    } else {    
      $ts = strtotime($date);
      if (date('H:i:s', $ts) == '00:00:00')
        $e->_value = date('Ymd', $ts);
      else
        $e->_value = date("YmdHis", $ts);
    }
    return $e;
    
  }
  static function fromNow() {
    return self::fromDate();
  }
}
