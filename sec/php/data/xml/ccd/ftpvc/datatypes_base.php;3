    $e = static::fromUgid($user->userGroupId);  // TODO
    $e->_extension = $user->userId;
    return $e;
  }
  //
  private static function fromUgid($ugid) {
    $root = '2.16.840.1.113883.19.5';  // TODO
    return static::from($root);
  }
