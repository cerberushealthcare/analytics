    }
  },
  //
  Procedures:{
    /*
     * @arg int cid
     * @callback(Proc[])
     */
    getAll:function(cid, callback) {
      Ajax.getr(Ajax.Facesheet._SVR, 'getProcs', cid, Procedures, callback);
    },
    /*
     * @arg int procId
     * @callback(Proc)
     */
    get:function(procId, callback) {
      Ajax.getr(Ajax.Facesheet._SVR, 'getProc', procId, Proc, callback);
    },
    /*
     * @arg Proc proc
     * @callback(Proc)
     */
    save:function(rec, callback, onerror) {
      Ajax.postr(Ajax.Facesheet._SVR, 'saveProc', rec, Proc, callback, onerror);
    },
    /*
     * @arg int procId
     * @arg ProcResult result
     * @callback()
     */
    saveResult:function(procId, result, callback, onerror) {
      Ajax.post(Ajax.Facesheet._SVR, 'saveResult', {'procId':procId,'result':result}, callback, onerror);
    },
    /*
     * @arg int id
     * @callback(int) id
     */
    deleteProc:function(id, callback) {
      Ajax.get(Ajax.Facesheet._SVR, 'deleteProc', id, callback);
    },
    /*
     * @arg int id
     * @callback(int) id
     */
    deleteResult:function(id, callback) {
      Ajax.get(Ajax.Facesheet._SVR, 'deleteResult', id, callback);
    }
  },
  //
  Documentation:{
    /*
     * @arg int cid
     * @callback(DocStub[])
     */
    getAll:function(cid, callback) {
      Ajax.get(Ajax.Facesheet._SVR, 'getDocStubs', cid, callback);
    },
    /*
     * @arg DocStub rec
     * @callback(Rec)  
     */
