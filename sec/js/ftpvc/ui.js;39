/**
 * Class 
 */
Class = {
  /*
   * Define constructor function and prototype/static methods
   */
  define:function(constructor, prototype, statics) {
    constructor.prototype = prototype;
    Class.augment(constructor, null, statics);
    return constructor;
  },
  /*
   * Augment existing object with additional prototype/static methods
   */
  augment:function(object, prototype, statics) {
    for (var name in prototype)
      object.prototype[name] = prototype[name];
    for (var name in statics)
      object[name] = statics[name];
    return object;
  }
}
/**
 * Object
 */
Object = Class.augment(Object, null, 
  {  // statics
    is:function(e) {
      return e && typeof e === 'object';
    },
    isUndefined:function(e) {
      var u;
      return e === u;
    },
    clone:function(e) {
      var n = Array.is(e) ? [] : {};
      for (var i in e) 
        if (Object.is(e[i]))
          if (Html.is(e[i])) 
            n[i] = e[i];
          else 
            n[i] = Object.clone(e[i]);
        else
          n[i] = e[i];
      return n;
    }
  });
/**
 * Map
 */
Map = {
  is:function(e) {
    return e && typeof e === 'object' && ! Array.is(e);
  },
  /*
   * @arg map/array e
   * @arg string keyFid (optional, to use e[i].keyFid as key; by default, key=i)
   * @arg string valueFid (optional, to use e[i].valueFid as value; by default, value=e[i])
   * @return {key:value,..}
   */
  from:function(e, keyFid, valueFid) {
    var m = {}, key, value;
    if (Array.is(e)) {
      for (var i = 0, j = e.length; i < j; i++) {
        key = (keyFid) ? e[i][keyFid] : i;
        value = (valueFid) ? e[i][valueFid] : e[i];
        m[key] = value;
      }
    } else if (Map.is(e)) {
      for (var i in e) {
        key = (keyFid) ? e[i][keyFid] : i;
        value = (valueFid) ? e[i][valueFid] : e[i];
        m[key] = value;
      }
    }
    return m;
  }
}
/**
 * String
 */
String = Class.augment(String, 
  {  // prototype
    plural:function(amt) {
      var noun = (amt == 1) ? this : this + 's';
      return amt + ' ' + noun;
    },
    beginsWith:function(e) {
      return (this.substr(0, e.length) == e);
    },
    addSlashes:function() {
      return this.replace(/([\\"'])/g, "\\$1").replace(/\0/g, "\\0");
    }
  },{  // statics
    is:function(e) {
    return e && typeof e == 'string';
    },
    isBlank:function(e) {
      return e == null || String.trim(e).length == 0;
    },
    from:function(e) {
      return e + '';
    },
    trim:function(e) {
      return (e != null) ? (e + '').replace('\xa0',' ').replace(/^\s+|\s+$/g, "") : null;
    },
    denull:function(e) {
      return (e == null) ? '' : e + '';
    },
    nullify:function(e) {
      return (e == null) ? null : (trim(e + '') == '') ? null : e;
    },
    toInt:function(e) {
      return (isNaN(e)) ? 0 : parseInt(e, 10);
    },
    toFloat:function(e) {
      return (isNaN(e)) ? 0 : parseFloat(e);
    },
    zpad:function(i) {
      return (i < 10) ? '0' + num : num;
    },
    rnd:function(prefix, len) {
      len = len || 8;
      prefix = prefix || '';
      return prefix + (Math.random() + '').substr(2, len);
    },
    px:function(i) {
      return (i === null) ? '' : (isNaN(i)) ? i : parseInt(i, 10) + 'px';
    }  
  });
/**
 * Boolean
 */
Boolean = Class.augment(Boolean, null,
  {  // statics
    toInt:function(e) {
      return (e) ? 1 : 0;
    },
    fromInt:function(e) {
      return (e == 1);
    },
    toString:function(e) {
      return (e) ? 'true' : 'false'
    },
    fromString:function(e) {
      return (e.toUpperCase() == 'TRUE');
    }
  });
/**
 * Array
 */
Array = Class.augment(Array, 
  {  // prototype
    isEmpty:function() {
      return Array.isEmpty(this);
    },
    append:function(e) {
      if (Array.is(e))
        Array.forEach(this,
          function(ei) {
            this.push(ei);
          }); 
      else
        this.push(e);
    },
    pushIfNotNull:function(e) {
      if (e != null)
        this.push(e);
    },
    find:function(item) {
      for (var i = 0, l = this.length; i < l; i++) 
        if (this[i] == item) 
          return i;
      return -1;
    },
    has:function(item) {
      return this.find(item) > -1;
    }
  },{  // statics
    is:function(e) {
      return e != null && e.constructor == Array;
    },
    isEmpty:function(array) {
      return array == null || array.length == 0;
    },
    from:function(e) {
      if (Array.is(e))
        return e;
      if (e == null) 
        return [];
      if (Map.is(e)) {
        var a = [];
        for (var i in e)
          a.push(e[i]);
        return a;
      } else {
        return [e];
      }
    },
    forEach:function(array, fn) {
      if (array)
        for (var i = 0, l = array.length; i < l; i++) 
          fn.call(array, array[i], i);
    }
  });
/**
 * Math
 */
Math = Class.augment(Math, null, 
  {  // statics
    sgn:function(x) {
      return (x > 0) | -(x < 0);      
    },
    isEven:function(i) {
      return (i % 2 == 0);
    },
    larger:function(x1, x2) {
      return (x1 > x2) ? x1 : x2;  
    },
    largest:function(array) {
      var max;
      Array.forEach(array,
        function(x) {
          if (max == null || x > max) 
            max = x;
        });
      return max;
    },
    smaller:function(x1, x2) {
      return -Math.larger(-x1, -x2);      
    },
    smallest:function(array) {
      var min;
      Array.forEach(array,
        function(x) {
          if (min == null || x < min) 
            min = x;
        });
      return min;
    }
  });
/**
 * Function
 */
Function = Class.augment(Function, null,
  {  // statics
    is:function(e) {
      try {  
        return /^\s*\bfunction\b/.test(e);  
      } catch (e) {
        return false;  
      }    
    },
    from:function(e) {
      return (Function.is(e)) ? e : new Function(e);
    }
  });
/**
 * Html
 */
function _$(e) {
  if (String.is(e))
    e = document.getElementById(e);
  return Html.extend(e);
}
Html = {
  is:function(e) {
    return e && e.nodeName != null;
  },
  create:function(tag, cls) {
    var e = document.createElement(tag);
    if (cls)
      e.className = cls;
    Html.extend(e);
    return e;
  },
  getTag:function(tag, parent, createIfNull) {
    var tags = parent.getElementsByTagName(tag);
    if (tags && tags.length > 0)
      return Html.extend(tags[0]);
    else 
      return (createIfNull) ? parent.append(tag) : null;
  },
  extend:function(e) {
    if (e && ! e._extended) {
      Class.augment(e, null, Html._Methods);
      e._extended = Html;
      switch (e.tagName) {
        case 'TABLE':
          Class.augment(e, null, Html.Table._Methods);
          break;
        case 'TBODY':
        case 'THEAD':
          Class.augment(e, null, Html.Table._TbodyMethods);
          break;
      }
    }
    return e;
  },
  _Methods:{
    augment:function(statics) {
      if (statics) {
        Class.augment(this, null, statics);
        if (statics.init) 
          statics.init.call(this);
      }
      return this;
    },
    append:function(e, fid) {
      if (String.is(e)) 
        e = Html.create(e);
      this.appendChild(_$(e));
      if (fid)
        this[fid] = e;
      return e;
    },
    into:function(e) {
      if (e)
        e.appendChild(this);
      return this;
    },
    add:function(e, fid) {
      if (e)
        this.append(e, fid);
      return this;
    },
    clean:function() {
      while (this.hasChildNodes()) 
        this.removeChild(this.lastChild);
      return this;
    },
    text:function(s) {
      this.innerText = String.denull(s);
      return this;
    },
    hide:function() {
      this.style.display = 'none';
      return this;
    },
    show:function() {
      this.style.display = '';
      return this;
    },
    showIf:function(test) {
      if (test) 
        this.show();
      else
        this.hide();
    },
    hideIf:function(test) {
      this.showIf(! test);
    },
    getPos:function() {
      var l = 0, t = 0;
      var e = this;
      if (e.offsetParent) {
        do {
          l += e.offsetLeft;
          t += e.offsetTop;
        } while (e = e.offsetParent);
      }
      return {'left':l,'top':t};
    },
    getDim:function() {
      var dis = this.style.display;
      if (this.style.get('display') == 'none')
        this.style.display = 'block';
      var h = this.offsetHeight;
      var w = this.offsetWidth;
      this.style.display = dis;
      return {'height':h,'width':w};
    },
    setLeft:function(i) {
      this.style.left = String.px(i);
      return this;
    },
    setTop:function(i) {
      this.style.top = String.px(i);
      return this;
    },
    setHeight:function(i) {
      this.style.height = (i == null) ? 'auto' : String.px(i);
      return this;
    },
    setWidth:function(i) {
      this.style.width = (i == null) ? 'auto' : String.px(i);
      return this;
    },
    setPosition:function(s) {
      this.style.position = s;
      return this;
    },
    setFocus:function(e) {
      try {
        e.focus();
        e.select();
      } catch (ex) {}
      return this;
    },
    working:function(e) {
      overlayWorking(e, this);
      if (Function.is(e)) 
        setTimeout(e, 1);
      return this;
    },
    hasClass:function(cls, startsWith) {
      var extra = (startsWith) ? '*' : '(?:$|\\s)';  
      var hasClassName = new RegExp('(?:^|\\s)' + cls + extra);
      var ec = this.className;
      if (ec && ec.indexOf(cls) != -1 && hasClassName.test(ec)) 
        return true; 
    },
    setClass:function(cls) {
      this.className = cls;
      return this;
    },
    addClass:function(cls) {
      if (! this.hasClass(cls))
        this.className = String.trim(this.className + ' ' + cls);
      return this;
    },
    removeClass:function(cls) {
      this.className = String.trim(this.className.replace(cls, ''));
      return this;
    },
    addClassIf:function(cls, test) {
      if (test)
        this.addClass(cls);
      else
        this.removeClass(cls);
      return this;
    },
    setUnselectable:function() {
      this.unselectable = 'on';
      return this;
    }
  }
};
Html.Window = {
  getEvent:function(e) {
    if (e) {
      e.srcElement = e.target;
      return e;
    } else {
      return window.event;
    }
  },
  getScrollTop:function() {
    if (typeof pageYOffset != 'undefined')
      return pageYOffset;
    else
      return document.documentElement.scrollTop;
  },
  getScrollLeft:function() {
    if (typeof pageXOffset != 'undefined')
      return pageXOffset;
    else
      return document.documentElement.scrollLeft;
  },
  getViewportDim:function() {
    var h, w;
    if (typeof window.innerWidth != 'undefined') { 
      w = window.innerWidth;
      h = window.innerHeight;
    } else if (typeof document.documentElement != 'undefined' && typeof document.documentElement.clientWidth != 'undefined' && document.documentElement.clientWidth != 0) {
      w = document.documentElement.clientWidth;
      h = document.documentElement.clientHeight
    } else {
      w = document.getElementsByTagName('body')[0].clientWidth;
      h = document.getElementsByTagName('body')[0].clientHeight;
    }
    return {'height':h,'width':w};
  },
  attachEvent:function(event, fn, o) {
    o = o || window;
    if (window.addEventListener)  
      o.addEventListener(event, fn, false);
    else 
      o.attachEvent('on' + event, fn);
  },
  detachEvent:function(event, fn, o) {
    o = o || window;
    if (window.removeEventListener)
      o.removeEventListener(event, fn, false);
    else 
      o.detachEvent('on' + event, fn);
  },
  setOnFocus:function(fn) {
    if (typeof(document.onfocusin) == 'object') 
      document.onfocusin = fn;
    else
      window.onfocus = fn;
  },
  setOnBlur:function(fn) {
    if (typeof(document.onfocusout) == 'object') 
      document.onfocusout = fn;
    else
      window.onblur = fn;
  },
  execScript:function(str) {
    if (window.execScript)
      window.execScript(str);
    else
      with (window) 
        window.eval(str);
  },
  append:function(e) {
    $('bodyContainer').appendChild(_$(e));
    return e;
  }
}
Html.Anchor = {
  is:function(e) {
    return Html.is(e) && e.tagName == 'A';
  },
  create:function(cls, text, fn, statics) {
    var e = Html.create('a', cls);
    e.href = 'javascript:';
    e.text(text);
    if (fn) 
      e.onclick = fn;
    return e.augment({
      tooltip:function(text) {
        e.title = text;
        return e;
      }
    }).augment(statics);
  }   
}
Html.Br = {
  create:function() {
    return Html.create('br');
  }
}
Html.Div = {
  is:function(e) {
    return Html.is(e) && e.tagName == 'DIV';
  },
  create:function(cls, statics) {
    var e = Html.create('div', cls);
    return e.augment({
      html:function(h) {
        e.innerHTML = h;
        return e;
      }}).augment(statics);
  }
}
Html.Image = {
  create:function(cls, src, height, width, alt) {
    var e = Html.create('img', cls);
    e.src = String.denull(src);
    e.setHeight(height);
    e.setWidth(width);
    e.alt = String.denull(alt);
    return e;
  }
}
Html.H1 = {
  create:function(text) {
    var e = Html.create('h1');
    e.text(text);
    return e;
  }
}
Html.H2 = {
  create:function(text) {
    var e = Html.create('h2');
    e.text(text);
    return e;
  }
}
Html.H3 = {
  create:function(text) {
    var e = Html.create('h3');
    e.text(text);
    return e;
  }
}
Html.Input = {
  create:function(type, cls, value, name, statics) { 
    var e = Html.create('input', cls);
    e.type = type;
    e.value = String.denull(value);
    e.name = String.denull(name);
    return e.augment(statics);
  }
}
Html.InputText = {
  create:function(cls, value, name, statics) {
    var e = Html.Input.create('text', cls, value, name);
    return e.augment({
      onkeypress:function() {
        if (e.onkeypresscr && event.keyCode == 13)
          e.onkeypresscr();
      },
      setSize:function(i) {
        if (i)
          e.size = i;
        return e;
      }
    }).augment(statics);
  }
}
Html.InputCheck = {
  create:function(cls, value, name, statics) {
    var e = Html.Input.create('checkbox', cls, value, name);
    return e.augment({
      setCheck:function(value) {
        e.checked = (value == true);
        return e;
      }
    }).augment(statics);
  }
}
Html.InputButton = {
  create:function(cls, value, statics) {
    return Html.Input.create('button', cls, value, statics);
  }
}
Html.InputHidden = {
  create:function(value, name, statics) {
    return Html.Input.create('hidden', null, value, name, statics);
  }
}
Html.Label = {
  create:function(cls, text, statics) {
    var e = Html.create('label', cls);
    e.text(text);
    return e.augment(statics);
  }
}
Html.Span = {
  create:function(cls, text, statics) {
    var e = Html.create('span', cls);
    e.text(text);
    return e.augment({
      html:function(h) {
        e.innerHTML = h;
        return e;
      }}).augment(statics);
  }
}
Html.Table = {
  is:function(e) {
    return Html.is(e) && e.tagName == 'TABLE';
  },
  create:function(cls, statics) {
    var t = Html.create('table', cls).augment(statics);
    return t;
  },
  _Methods:{
    tbody:function() {
      if (this._tbody == null) 
        this._tbody = Html.getTag('tbody', this, true);
      return this._tbody; 
    },
    thead:function() {
      if (this._thead == null)
        this._thead = Html.getTag('thead', this, true);
      return this._thead; 
    }
  },
  _TbodyMethods:{
    tr:function(cls) {
      this._tr = Html.create('tr', cls).into(this);
      return Html.Table._TbodyMethods._trAppender(this._tr);
    },
    _trAppender:function(tr) {
      return {
        td:function(e, cls) {
          this._cell = Html.create('td', cls).into(tr);
          this._appendOrText(e);
          return this;
        },
        th:function(e, cls) {
          this._cell = Html.create('th', cls).into(tr);
          this._appendOrText(e);
          return this;
        },
        w:function(i) {
          this._cell.setWidth(i);
          return this;
        },
        rowspan:function(i) {
          this._cell.rowSpan = i;
          return this;
        },
        colspan:function(i) {
          this._cell.colSpan = i;
          return this;
        },
        _tr:function() {
          return tr;
        },
        _appendOrText:function(e) {
          if (Html.is(e))
            this._cell.append(e);
          else 
            this._cell.text(e);
        }
      }
    }
  }
}
Html.Ul = {
  create:function(cls, statics) {
    return Html.create('ul', cls).augment(statics);
  }
}
